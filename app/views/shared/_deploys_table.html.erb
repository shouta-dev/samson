<section class="tabs">
  <table class="table">
    <thead>
      <tr>
        <% unless @project %>
          <th>Project</th>
        <% end %>

        <th>When</th>
        <th>Who</th>
        <% unless @stage %>
          <th>Stage</th>
        <% end %>
        <th>Status</th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= render partial: "/projects/deploy", collection: @deploys %>
    </tbody>
  </table>

  <%= paginate @deploys if @deploys.respond_to?(:total_pages) %>
</section>
